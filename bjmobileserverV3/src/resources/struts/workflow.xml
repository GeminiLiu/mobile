<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="sheet" extends="eoms" namespace="/sheet">
		<action name="*" method="{1}" class="sheetQueryAction">
			<result name="custom">/workflow/sheet/query/${target}.jsp</result>
		</action>
	</package>
	
	<package name="sheetInfo" extends="eoms" namespace="/sheetInfo">
		<action name="*" method="{1}" class="workSheetAction">
			<result name="custom">/workflow/sheet/query/${target}.jsp</result>
		</action>
	</package>
	
	<package name="/portal" extends="eoms" namespace="/portal">
		<action name="*" method="{1}" class="portalAction">
			<result name="index">/login.jsp</result>
			<result name="main">/workflow/sheet/query/main.jsp</result>
		</action>
	</package>
	
	<package name="mobile" extends="eoms" namespace="/mobile">
		<action name="*" method="{1}" class="mobileAction">
			<result name="success">/mobile/${1}.jsp</result>
			<result name="custom">/mobile/${target}.jsp</result>
		</action>
	</package>
	
</struts>
