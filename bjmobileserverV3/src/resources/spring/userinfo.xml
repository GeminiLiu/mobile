<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-autowire="byName" default-lazy-init="true">
	<bean id="userManagerService" class="com.ultrapower.mobile.service.impl.UserManagerImpl" />
	<bean id="userManagerDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.mobile.model.UserInfo</value>
		</property>
	</bean>
	
	<bean id="userOnlineManagerService" class="com.ultrapower.mobile.service.impl.UserOnlineManagerImpl" />
	<bean id="userOnlineManagerDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.mobile.model.UserOnline</value>
		</property>
	</bean>
</beans>