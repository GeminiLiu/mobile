<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
   "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>ultra</short-name>
	<uri></uri>
	<tag><!-- 专业的相关信息-->
		<name>major</name>
		<tag-class>cn.com.ultrapower.eoms.util.tag.MajorSelect</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>name</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>addPlease</name>
			<required>false</required>
		</attribute>
	</tag>
	<tag><!-- 代维公司-->
		<name>agentCor</name>
		<tag-class>cn.com.ultrapower.eoms.util.tag.AgentCorporation</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>name</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>addPlease</name>
			<required>false</required>
		</attribute>
		
	</tag>
	<tag><!-- 河北省各地市-->
		<name>city</name>
		<tag-class>cn.com.ultrapower.eoms.util.tag.HeBeiCity</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>name</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>addPlease</name>
			<required>false</required>
		</attribute>
	</tag>
	<tag><!-- 普通的下拉菜单，传入选项，逗号分割-->
		<name>select</name>
		<tag-class>cn.com.ultrapower.eoms.util.tag.GerneralSelect</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>name</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>items</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>addPlease</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
		</attribute>
	</tag>
</taglib>
