	<sql name="accidenttatisticlink" datasource="Ultranms">
		<item dbms="All"><![CDATA[
select
distinct(t.c1),t.c700000001,t.c700000003 as 工单编号,
t.c700000006 as 建单时间,t.c700000004 as 建单人,
t.c700000011 as 主题,t.c700000015 as 紧急程度,
t.c700000014 as 所属专业,t.c700000010 as 工单状态
from {600} t,{500} item,{200} u,{300} g,{400} gu
where
item.c802800005=t.c802000018
and u.c1=gu.c620000028
and g.c1=gu.c620000027
and t.c700000005=u.c630000001
{2}
{3}
{4}
{6}
--and g.c630000018=trim('值班组1')
--and (t.c802000018 ='电源设备')
		]]></item>
	</sql>
</sql-group>

