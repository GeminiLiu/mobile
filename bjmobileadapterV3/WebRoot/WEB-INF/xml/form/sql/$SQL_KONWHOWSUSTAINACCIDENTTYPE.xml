	<sql name="knowhowsustainaccidenttype" datasource="Ultranms">
		<item dbms="All"><![CDATA[
select
g.c630000018 as 派发部门,
t.c802000004 as 问题类型,
count(distinct t.c1) as 数量
from {100} t,{200} u,{300} g,{400} gu
where
t.c700000005=u.c630000001
and u.c1=gu.c620000028
and gu.c620000027=g.c1
and t.c700000010='已关闭'
{2}
{3}
{5}
{6}
group by g.c630000018,t.c802000004  
		]]></item>
	</sql>
	
		<sql name="knowhowsustainquestiontype" datasource="Ultranms">
		<item dbms="All"><![CDATA[
             select distinct c802000004 from {400} where c802000004 is not null
		]]></item>
	</sql>
	    
	    <sql name="knowhowsustaingrouptype" datasource="Ultranms">
		<item dbms="All"><![CDATA[
             select distinct g.c630000018 as 派单部门 from {200} g where 1=1 {2}
		]]></item>
	</sql>
