<?xml version="1.0" encoding="UTF-8"?>
<sql-group>
	<doc>
		<author>lihaoyuan</author> 
		<date>2008年10月21日</date> 
		<description>测试SQL</description> 		
	</doc>
	<sqlquery name="query.test.001">
		<select>
		<![CDATA[ 
   			  select 
   			  /*COUNT*/   
   			  base.c1 as baseid,
      		  c700000001      as baseschema,
      		  c700000002      as basename,
       		  c700000003      as basesn,
       		  c700000018      as BaseDealOutTime,
       	      c700000010      as BaseStatus,
       	      c700000011      as BaseSummary,
       	      c700000015      as BasePriority
       	      ,dp.c1           as processid,
              c700020020      as FlagActive,
              c700020019      as FlagType,
              c700020006      as AssgineeID,
              c700020010      as DealerID,
              c700020008      as GroupID
              /*COUNT*/
              from {WF:App_Base_bak} base,{WF:App_DealProcess} dp 
			  where base.c1 = dp.c700020001
				   and base.c700000001 = dp.c700020002
				   and c700020020=1 
				   $no_test$
				   $waitdeal$ 
				   $extend$
		  ]]> 
		</select>
		<customwhere>
			<parameterMap prepend="AND" name="no_test">
				<mapingFiled  prepend="AND" fieldName="basesn"  fieldType="4">
					<mapsql value="isnotnull">
						<![CDATA[
							 c700000003 like '%#basesn#%'
						 ]]>  			
					</mapsql>
				</mapingFiled>
				<mapingFiled  prepend="AND" fieldName="BaseSummary"  fieldType="4">
					<mapsql value="isnotnull">
						<![CDATA[
							 c700000011 like '%#BaseSummary#%'
						 ]]>  			
					</mapsql>
				</mapingFiled>
			</parameterMap>
			<parameterMap prepend="" name="extend">
					<mapingFiled  prepend="" fieldName="orderby"  fieldType="4">
						<mapsql value="isnotnull">
							<![CDATA[
								   order by #orderby#
							 ]]>
						</mapsql>
					</mapingFiled>
			</parameterMap>					
		</customwhere>
		<sqlother>
			<sqlpagination isavailability="true">
				<sqlpageSize>20</sqlpageSize>
			</sqlpagination>
			<sqldistinct isavailability="false">排重的列1,排重的列2</sqldistinct>
			<sqlSortFields isavailability="false">排序的列1,排序的列2</sqlSortFields>
		</sqlother>
		<PageQueryField>
			<field name="basesn">
				<fieldID>700000003</fieldID><!--查询字段ID-->
				<fieldEnglishname>basesn</fieldEnglishname><!--查询字段英文名，该英文名会做为我们查询条件-->
				<fieldChinesename>工单流水号</fieldChinesename><!--查询字段用于在界面显示的名称-->
				<fieldType>4</fieldType><!--查询字段的数据类型(4：字符等，与Remedy类型保持一致)-->
				<fieldDisplaytype>4</fieldDisplaytype><!--查询字段用于在界面显示的类型(4：字符/TEXT等，与Remedy类型保持一致)-->
				<fieldDisplaytypeValue></fieldDisplaytypeValue><!--查询字段用于在界面显示的值的范围(只有当是Select时有值：格式为"值+冒号+显示值+分号，"如：0:是;1:否;';)-->
				<fieldDefaultValue></fieldDefaultValue><!--查询字段用于在界面显示时的默认值-->
			</field>
			<field name="BaseSummary">
				<fieldID>700000003</fieldID><!--查询字段ID-->
				<fieldEnglishname>BaseSummary</fieldEnglishname><!--查询字段英文名，该英文名会做为我们查询条件-->
				<fieldChinesename>工单主题</fieldChinesename><!--查询字段用于在界面显示的名称-->
				<fieldType>4</fieldType><!--查询字段的数据类型(4：字符等，与Remedy类型保持一致)-->
				<fieldDisplaytype>4</fieldDisplaytype><!--查询字段用于在界面显示的类型(4：字符/TEXT等，与Remedy类型保持一致)-->
				<fieldDisplaytypeValue></fieldDisplaytypeValue><!--查询字段用于在界面显示的值的范围(只有当是Select时有值：格式为"值+冒号+显示值+分号，"如：0:是;1:否;';)-->
				<fieldDefaultValue></fieldDefaultValue><!--查询字段用于在界面显示时的默认值-->
			</field>
		</PageQueryField>
	</sqlquery>
	
</sql-group>
