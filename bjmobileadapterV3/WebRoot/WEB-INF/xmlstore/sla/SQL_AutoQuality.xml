<?xml version="1.0" encoding="UTF-8"?>
<sql-group>
	<doc>
		<author>wxdong</author> 
		<date>2010年8月17日</date> 
		<description>投诉工单自动质检配置查询</description> 		
	</doc>
	<sqlquery name="query.AutoQuality">
		<select>
		<![CDATA[ 
		select 
			/*COUNT*/
			*
			/*COUNT*/
			 from(
		select * from configAutoQuality t
		where 1=1 
		$basepara$ 
		) result 
		$extend$
		  ]]> 
		</select>
		<customwhere>
			<parameterMap prepend="AND" name="basepara">
				<mapingFiled  prepend="AND" fieldName="ConfigName"  fieldType="4">
					<mapsql value="isnotnull">
						<![CDATA[
							 CFG_NAME like '%#ConfigName#%'
						 ]]>
					</mapsql>
				</mapingFiled>
				<mapingFiled  prepend="AND" fieldName="ConfigBaseName"  fieldType="4">
					<mapsql value="isnotnull">
						<![CDATA[
							 CFG_BASENAME = '#ConfigBaseName#'
						 ]]>
					</mapsql>
				</mapingFiled>
				<mapingFiled  prepend="AND" fieldName="ConfigStatus"  fieldType="4">
					<mapsql value="isnotnull">
						<![CDATA[
							 CFG_STATUS = '#ConfigStatus#'
						 ]]>
					</mapsql>
				</mapingFiled>
			</parameterMap>
			<parameterMap prepend="EXTEND" name="extend">
					<mapingFiled  prepend="EXTEND" fieldName="orderby"  fieldType="4">
						<mapsql value="isnotnull">
							<![CDATA[
								   order by #orderby#
							 ]]>
						</mapsql>
					</mapingFiled>
			</parameterMap>
		</customwhere>
		<sqlother>
			<sqlpagination isavailability="true">
				<sqlpageSize>20</sqlpageSize>
			</sqlpagination>
			<sqldistinct isavailability="false">排重的列1,排重的列2</sqldistinct>
			<sqlSortFields isavailability="false">排序的列1,排序的列2</sqlSortFields>
		</sqlother>
		<PageQueryField>
			<field name="ConfigName">
				<fieldID>ConfigName</fieldID><!--查询字段ID-->
				<fieldEnglishname>ConfigName</fieldEnglishname><!--查询字段英文名，该英文名会做为我们查询条件-->
				<fieldChinesename>配置名称</fieldChinesename><!--查询字段用于在界面显示的名称-->
				<fieldType>4</fieldType><!--查询字段的数据类型(4：字符等，与Remedy类型保持一致)-->
				<fieldDisplaytype>4</fieldDisplaytype><!--查询字段用于在界面显示的类型(4：字符/TEXT等，与Remedy类型保持一致)-->
				<fieldDisplaytypeValue></fieldDisplaytypeValue><!--查询字段用于在界面显示的值的范围(只有当是Select时有值：格式为"值+冒号+显示值+分号，"如：0:是;1:否;';)-->
				<fieldDefaultValue></fieldDefaultValue><!--查询字段用于在界面显示时的默认值-->
			</field>
			<field name="ConfigBaseName">
				<fieldID>ConfigBaseName</fieldID><!--查询字段ID-->
				<fieldEnglishname>ConfigBaseName</fieldEnglishname><!--查询字段英文名，该英文名会做为我们查询条件-->
				<fieldChinesename>工单类别</fieldChinesename><!--查询字段用于在界面显示的名称-->
				<fieldType>6</fieldType><!--查询字段的数据类型(4：字符等，与Remedy类型保持一致)-->
				<fieldDisplaytype>6</fieldDisplaytype><!--查询字段用于在界面显示的类型(4：字符/TEXT等，与Remedy类型保持一致)-->
				<fieldDisplaytypeValue>个人投诉工单:个人投诉工单;集团投诉工单:集团投诉工单;</fieldDisplaytypeValue><!--查询字段用于在界面显示的值的范围(只有当是Select时有值：格式为"值+冒号+显示值+分号，"如：0:是;1:否;';)-->
				<fieldDefaultValue></fieldDefaultValue><!--查询字段用于在界面显示时的默认值-->
			</field>
			<field name="ConfigStatus">
				<fieldID>ConfigStatus</fieldID><!--查询字段ID-->
				<fieldEnglishname>ConfigStatus</fieldEnglishname><!--查询字段英文名，该英文名会做为我们查询条件-->
				<fieldChinesename>启动状态</fieldChinesename><!--查询字段用于在界面显示的名称-->
				<fieldType>6</fieldType><!--查询字段的数据类型(4：字符等，与Remedy类型保持一致)-->
				<fieldDisplaytype>6</fieldDisplaytype><!--查询字段用于在界面显示的类型(4：字符/TEXT等，与Remedy类型保持一致)-->
				<fieldDisplaytypeValue>1:是;0:否;</fieldDisplaytypeValue><!--查询字段用于在界面显示的值的范围(只有当是Select时有值：格式为"值+冒号+显示值+分号，"如：0:是;1:否;';)-->
				<fieldDefaultValue></fieldDefaultValue><!--查询字段用于在界面显示时的默认值-->
			</field>
		</PageQueryField>
	</sqlquery>
</sql-group>
