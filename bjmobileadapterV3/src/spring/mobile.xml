<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-autowire="byName" default-lazy-init="true">
	
	<bean id="mobileManagerService" class="com.ultrapower.eoms.workflow.notice.manager.MobileManagerImpl" />
	<bean id="noticeDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.eoms.workflow.notice.model.Notice</value>
		</property>
	</bean>
	<!-- 服务端接口 为手机端提供服务 -->
	<bean id="queryDealListService" class="com.ultrapower.eoms.mobile.interfaces.querydeallist.manager.QueryDealListManager" />
	<bean id="openFormService" class="com.ultrapower.eoms.mobile.interfaces.openform.manager.OpenFormManager" />
	<bean id="initActionService" class="com.ultrapower.eoms.mobile.interfaces.initaction.manager.InitActionManager" />
	<bean id="formCommitService" class="com.ultrapower.eoms.mobile.interfaces.formcommit.manager.ForCommitManager" />
	<bean id="dealDescService" class="com.ultrapower.eoms.mobile.interfaces.dealdesc.manager.DealDescManager" />
	<bean id="syncAttachService" class="com.ultrapower.eoms.mobile.interfaces.syncattach.manager.SyncAttachManager" />
	<bean id="downAttachService" class="com.ultrapower.eoms.mobile.interfaces.downattach.manager.DownAttachManager" />
	
	<bean id="mobileAuthorizeService" class="com.ultrapower.eoms.mobile.interfaces.authorize.manager.AuthorizeManager" />
	<bean id="mobileTemplateService" class="com.ultrapower.eoms.mobile.interfaces.template.manager.TemplateManager" />
	<bean id="mobileAssignTreeService" class="com.ultrapower.eoms.mobile.interfaces.assigntree.manager.AssignTreeManager" />
	
	<!-- 工单模板 -->
	<bean id="mobileTemplateProvider" class="com.ultrapower.eoms.mobile.manager.TemplateProvider"></bean>
	<bean id="mobileTemplateAction" class="com.ultrapower.eoms.mobile.web.MobileTemplateAction"></bean>


</beans>